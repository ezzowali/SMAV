    <div class="row">

      
      <div class="col-md-4 mb-3">

        <input type="text" class=" effect-16" name="fullName" placeholder="الاسم الثلاثي " required 
        oninvalid="this.setCustomValidity('الرجاء تعبئة هذا الحقل ');" onchange="try{setCustomValidity('')}catch(e){};" x-moz-errormessage="الرجاء تعبئة هذا الحقل">
        <span class="focus-border"></span>

       
      </div>
    </div>


   




  
  


    <div class="row">
  <div class="col-md-4 mb-3 " >
    <input type="text" class="effect-16" name="jopTitle" value="" placeholder="الوظيفة"
