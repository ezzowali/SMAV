      <form action="/sign_up_Individuals" method="POST" enctype="multipart/form-data">

    <div class="frontside">
      <div class="card " >
          <div class="card-body text-center">
            <h4 class="card-title fas fa-user-circle" style="color: #0086d3;"> <i class=" "></i>  المعلومات الشخصية </h4>

   
            <div class="row">

        
             
             
        
        
   
        
        
                <div class="col-md-4 mb-3">
        
        

                  <input type="text" class="effect-16" id="firstName_En" name="firstName_En" placeholder="First Name" value="" required>
                  <span class="focus-border"></span>
                </div>
                <div class="col-md-4 mb-3" style="color: black;">
                  <input type="text" class="effect-16" id="middleName_En" name="middleName_En" placeholder="Middle Name" value="" required>
                  <span class="focus-border"></span>
        
                </div>
        
        
                <div class="col-md-4 mb-3" style="color: black;">
                  <input type="text" class="effect-16" id="lastName_En" name="lastName_En" placeholder="Last Name" value="" required>
                  <span class="focus-border"></span>
                </div>


        
                <div class="col-md-4 mb-3" style="color: black;" >

                <input type="number"  class="effect-16" min="500000000" name="phone" value="" placeholder=" Phone" required>
                <span class="focus-border"></span> 
            </div>

            <div class="col-md-4 mb-3" style="color: black;">

              <input type="email" class="effect-16" name="email" value="" placeholder="Email  " required>
              <span class="focus-border"></span>   
          </div>
        
        

            <div class="col-md-4 mb-3" style="color: black;">
              <select name="gender" class="select effect-16" required>
                <option value="">--  Gender  --</option>
                <option value="male">male</option>
                <option value="female">female</option>
            
                </select>
                <span class="focus-border"></span>
               </div>
   
         
            
            
            <div class="col-md-4 mb-3" style="color: black;">

            
              <input type="number" class="effect-16" id="age" name="age" placeholder="Age" min="14" max="100" required>
              <span class="focus-border"></span>
            </div>
        


            <div class="col-md-6 mb-3" style="color: black;">
              <select name="city" class="select effect-16" onchange="CheckingOther_city(this.value);"   required>
                <option value="">--  city  --</option>
                <option value="Riyadh">Riyadh</option>
                <option value="Makkah">Makkah</option>

                <option value="jeddah">jeddah</option>
                <option value="Dammam">Dammam</option>


                <option value="Abha">Abha</option>
                <option value="Taif"> Taif</option>


                <option value="Qassim">Qassim</option>
                <option value="Jazan">Jazan</option>


                <option value="Medinah">Medinah</option>
                <option value="Eastern">Eastern</option>



                <option value="Najran">Najran</option>
                <option value="Al-Baha">Al-Baha</option>


                <option value="Tabouk">Tabouk</option>
                <option value="Aseer">Aseer</option>


                <option value="other">other</option>
                
            
                </select>

                <input type="text" id="other_id_city" name="city" style='display:none;'/>
                <span class="focus-border"></span>
               </div>
        
         
   

        
                  </div>
        </div>
      </div>



      </div>


      <div class="frontside" >
        <div class="card " style="color: black;" >

            <div class="card-body  text-center" style="background-color: white;">


              <h4 class="card-title fas fa-university" style="color: #0086d3;">  المعلومات الأكاديمية   </h4>
  
            
              <div class="row">

                <div class="col-md-4 mb-3">
              
                  <input type="text" class="effect-16" name="university" placeholder="University or workplace" value="" required>
                  <span class="focus-border"></span>
                </div>
               
   
      
 <!-- <input type="text" name="city" list="citynames">
<datalist id="citynames">
  <option value="Boston">
  <option value="Cambridge">
</datalist> -->


                <div class="col-md-3 mb-3">
                  <select name="EducationLevel" class="select effect-16"  onchange='CheckingOther_eduLev(this.value);'
                  required>
                    <option value="">-- Education Level   --</option>
                    <option value="1st year student"> 1st year student </option>
          <option value="2nd year student">2nd year student </option>
      <option value="3rd year student"> 3rd year student </option>
      <option value="4th year student ">  4th year student </option>

      <option value="5th year student">5th year student </option>
      <option value="6th year student"> 6th year student </option>
      <option value="fresh graduate">  fresh graduate </option>

