      <option value="R2">R2 </option>


      <option value="R3">R3 </option>
      <option value="R4"> R4 </option>
      <option value="Specialist ">Specialist  </option>

      <option value="consultant ">consultant  </option>
      <option value="other"> other </option>

      <input type="text" id="other_id_EducationLevel" name="EducationLevel" style='display:none;'/>



    </select>
    <span class="focus-border"></span>
                </div>
                

      
                <div class="col-md-3 mb-3">
                  <select name="Specialty" class="select effect-16" required onchange='CheckingOther_spe(this.value);'>
                    <option value="">-- Specialty    --</option>
                    <option value="medicine "> medicine   </option>
          <option value="dentistry ">dentistry   </option>
      <option value="pharmacy"> pharmacy  </option>
      <option value="nursing ">  nursing  </option>
      <option value="public health"> public health  </option>
      <option value="Applied Medical Sciences">Applied Medical Sciences  </option>
      <option value="other"> other  </option>
      </select>

      <input type="text" id="other_id_Specialty" name="Specialty" style='display:none;'/>

      <span class="focus-border"></span>
                </div>

      
        <div class="col-md-4 mb-3">
            <input type="text" class="effect-16" name="SCFHS" value="" placeholder="رقم تصنيف الهيئة" required>
            <span class="focus-border"></span>
        </div>


        <div class="col-md-3 mb-3">
          <select name="times" class="select effect-16" required>
            <option value="">-- How many times do you volunteer?    --</option>
            <option value="1 "> 1   </option>
  <option value="2 ">2   </option>
<option value="3"> 3  </option>
<option value="4 ">  4  </option>
